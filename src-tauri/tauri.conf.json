{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Sweesh",
  "version": "0.1.0",
  "identifier": "com.sweesh.app",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Sweesh",
        "width": 800,
        "height": 600,
        "resizable": true,
        "decorations": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "wix": {
        "upgradeCode": "8d9c9d38-4a2b-46d3-b6d7-6b5b9d0a1c2f"
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
